{
    "name": "qudecim/fast-di",
    "description": "The simple dependency injection container",
    "keywords": ["di", "dependency injection", "psr-11"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Yevgeniy Zaichenko",
            "email": "hackpsy@yandex.ru"
        }
    ],
    "autoload": {
        "psr-4": {
            "DI\\": "src/",
            "Example\\": "example/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TestApp\\": "tests/IntegrationTest/TestApp/",
            "IntegrationTest\\": "tests/IntegrationTest/"
        }
    },
    "require": {
        "php": ">=8.0",
        "psr/container": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2"
    }
}
